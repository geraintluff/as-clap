{
  "options": {
    "lib": [
      "../../@assemblyscript/wasi-shim/assembly",
      "./node_modules/@assemblyscript/wasi-shim/assembly"
    ],
    "use": [
      "ASC_WASI=1",
      "abort=wasi_abort",
      "trace=wasi_trace",
      "seed=wasi_seed",
      "console=wasi_console",
      "crypto=wasi_crypto",
      "performance=wasi_performance",
      "process=wasi_process",
      "Date=wasi_Date"
    ],
    "exportStart": "_start",
    "importMemory": false,
    "sharedMemory": false,
    "exportTable": true,
    "enable": ["simd", "relaxed-simd"],
    "transform": "./transform.js"
  },
  "targets": {
    "debug": {
      "debug": true
    },
    "release": {
      "optimizeLevel": 3,
      "shrinkLevel": 0,
      "converge": false,
      "noAssert": false
    }
  }
}
